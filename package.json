{
  "name": "applitools-for-selenium-ide",
  "version": "1.7.8",
  "repository": "git@github.com:applitools/applitools-for-selenium-ide.git",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "yarn build:page && webpack",
    "build:prod": "rm -rf build && env NODE_ENV=production yarn build",
    "build:page": "webpack --config webpack.config.page.babel.js",
    "test": "jest",
    "lint": "eslint webpack.config.babel.js src/ --ext .js --ext .jsx",
    "postinstall": "patch-package",
    "version": "node scripts/version.js && git add src/manifest.json",
    "upgrade:sdks": "node scripts/update-sdks.js",
    "release": "(./scripts/deploy.js && node scripts/gh-release.js && yarn release:amo && yarn release:cws);exit 0;",
    "release:amo": "cd build && web-ext-submit",
    "release:cws": "cd build && webstore upload --auto-publish"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.svg$": "<rootDir>/__mocks__/emptyString.js"
    },
    "testMatch": [
      "**/src/**/__test?(s)__/**/*.js?(x)"
    ],
    "testURL": "http://localhost"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-process-hrtime": "^1.0.0",
    "chrome-webstore-upload-cli": "^1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "grizzly": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.5",
    "patch-package": "^5.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.14.3",
    "putasset": "^4.3.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "semver": "^6.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^2.0.1",
    "web-ext-submit": "^3.1.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "@applitools/dom-capture": "^7.0.17",
    "@applitools/dom-snapshot": "^2.2.3",
    "@applitools/eyes-images": "^4.12.10",
    "@applitools/visual-grid-client": "^12.21.2",
    "buffer": "^5.2.1",
    "classnames": "^2.2.5",
    "fuse.js": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "js-yaml": "^3.13.1",
    "pngjs": "^3.3.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.6.1",
    "react-tooltip": "^3.9.2",
    "ua-parser-js": "^0.7.18",
    "webextension-polyfill": "0.3.1"
  },
  "resolutions": {
    "set-value": "^3.0.1",
    "lodash": "^4.17.12",
    "js-yaml": "^3.13.0",
    "braces": "^2.3.1",
    "mime": "^2.0.3",
    "clean-css": "^4.1.11"
  }
}
