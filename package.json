{
  "name": "applitools-for-selenium-ide",
  "version": "0.0.0",
  "repository": "git@github.com:applitools/applitools-for-selenium-ide.git",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "webpack",
    "build:prod": "rm -rf build && env NODE_ENV=production yarn build",
    "test": "jest",
    "test:webdriver": "selenium-side-runner tests/*.side",
    "lint": "eslint webpack.config.babel.js src/ --ext .js --ext .jsx",
    "postinstall": "patch-package"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.svg$": "<rootDir>/__mocks__/emptyString.js"
    },
    "testMatch": [
      "**/src/**/__test?(s)__/**/*.js?(x)"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-process-hrtime": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.5",
    "patch-package": "^5.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.14.3",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "@applitools/dom-capture": "^6.1.1",
    "@applitools/eyes-images": "^4.5.4",
    "@applitools/visual-grid-client": "^9.1.5",
    "buffer": "^5.0.8",
    "classnames": "^2.2.5",
    "fuse.js": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "js-yaml": "^3.12.1",
    "pngjs": "^3.3.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.6.1",
    "react-testing-library": "^5.4.0",
    "react-tooltip": "^3.9.2",
    "ua-parser-js": "^0.7.18",
    "webextension-polyfill": "0.3.1"
  }
}
