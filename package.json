{
  "name": "applitools-for-selenium-ide",
  "version": "1.10.0",
  "repository": "git@github.com:applitools/applitools-for-selenium-ide.git",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "yarn build:page && webpack",
    "build:prod": "rm -rf build && env NODE_ENV=production yarn build",
    "build:page": "webpack --config webpack.config.page.babel.js",
    "test": "jest",
    "test:code-export:csharp": "node tests/e2e/code-export/run-tests.js csharp-nunit",
    "test:code-export:csharp:vg": "node tests/e2e/code-export/run-tests.js csharp-nunit true",
    "test:code-export:java": "node tests/e2e/code-export/run-tests.js java-junit",
    "test:code-export:java:vg": "node tests/e2e/code-export/run-tests.js java-junit true",
    "test:code-export:javascript": "node tests/e2e/code-export/run-tests.js javascript-mocha",
    "test:code-export:javascript:vg": "node tests/e2e/code-export/run-tests.js javascript-mocha true",
    "test:code-export:python": "node tests/e2e/code-export/run-tests.js python-pytest",
    "test:code-export:python:vg": "node tests/e2e/code-export/run-tests.js python-pytest true",
    "test:code-export:ruby": "node tests/e2e/code-export/run-tests.js ruby-rspec",
    "test:code-export:ruby:vg": "node tests/e2e/code-export/run-tests.js ruby-rspec true",
    "lint": "eslint webpack.config.babel.js src/ --ext .js --ext .jsx",
    "postinstall": "patch-package",
    "version": "node scripts/version.js && git add src/manifest.json",
    "upgrade:sdks": "node scripts/update-sdks.js",
    "release": "(./scripts/deploy.js && node scripts/gh-release.js && yarn release:amo && yarn release:cws);exit 0;",
    "release:amo": "cd build && web-ext-submit",
    "release:cws": "cd build && webstore upload --auto-publish"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.svg$": "<rootDir>/__mocks__/emptyString.js"
    },
    "testMatch": [
      "**/src/**/__test?(s)__/**/*.js?(x)"
    ],
    "testURL": "http://localhost"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@seleniumhq/code-export": "^3.16.0",
    "@seleniumhq/side-model": "^3.15.0",
    "@seleniumhq/side-utils": "^3.16.0",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "browser-process-hrtime": "^1.0.0",
    "chrome-webstore-upload-cli": "^1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "grizzly": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^24.9.0",
    "patch-package": "^5.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.14.3",
    "putasset": "^4.3.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "semver": "^6.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^2.0.1",
    "web-ext-submit": "^3.1.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "@applitools/dom-capture": "^7.0.17",
    "@applitools/dom-snapshot": "^3.0.5",
    "@applitools/eyes-images": "^4.12.15",
    "@applitools/visual-grid-client": "^13.0.1",
    "buffer": "^5.2.1",
    "classnames": "^2.2.5",
    "fuse.js": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "js-yaml": "^3.13.1",
    "pngjs": "^3.3.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.6.1",
    "react-tooltip": "^3.9.2",
    "ua-parser-js": "^0.7.18",
    "webextension-polyfill": "0.3.1"
  },
  "resolutions": {
    "set-value": "^3.0.1",
    "lodash": "^4.17.12",
    "js-yaml": "^3.13.0",
    "braces": "^2.3.1",
    "mime": "^2.0.3",
    "clean-css": "^4.1.11",
    "mem": "6.0.1",
    "serialize-javascript": "2.1.2"
  }
}
